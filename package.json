{
  "name": "resources-saver-extension",
  "version": "2.0.0",
  "description": "Chrome extension for one click downloading all resources!",
  "main": "index.js",
  "repository": "git@github.com:up209d/ResourcesSaverExt.git",
  "author": "up209d <up209d@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn cp && parcel watch ./src/index.html --out-dir unpacked2x",
    "dev-serve": "yarn cp && parcel ./src/index.html --port 20987 --out-dir unpacked2x",
    "build": "yarn cp && parcel build ./src/index.html --out-dir unpacked2x",
    "cp": "yarn clean && mkdir -p unpacked2x && cp -r ./src/static/* unpacked2x",
    "clean": "rm -rf unpacked2x",
    "reset": "yarn clean && rm -rf ./.cache",
    "local": "rm -rf ./node_modules/parcel-plugin-local && yarn install --check-files && yarn reset"
  },
  "alias": {
    "app": "./src/app",
    "components": "./src/components"
  },
  "parcelExternals": [
    "fonts/**/*",
    "vendors/**/*"
  ],
  "dependencies": {
    "app": "^0.1.0",
    "context": "^0.0.1",
    "global": "^4.4.0",
    "lodash": "^4.17.15",
    "polished": "^3.4.1",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-spring": "^8.0.27",
    "reset-css": "^5.0.0",
    "store": "^2.0.12",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-local-collection": "file:./plugins",
    "sass": "^1.23.0"
  }
}
